package com.day3;
class Customer{
	 String name ;
}

public class CustomerCopyConstructor {
	public static void main(String[] args) {
		Customer c1 = new Customer();
		c1.name = "Preetti";
		
		Customer c2 = c1;
		
		c2.name = "Sachin";
		
		System.out.println(c1.name);
		System.out.println(c2.name);
	}
	
	
	

}
