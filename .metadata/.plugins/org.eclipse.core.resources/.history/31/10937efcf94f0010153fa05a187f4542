package com.day11;

import java.util.HashSet;
import java.util.Set;

public class ContactSyncApp {
	public static void main(String[] args) {
		
		String[] simContcts = {"Srinivas","Sravan","Jeevan","Jaya","Akash"};
		String[] googleContcts = {"Nisha","Nivetha","Lavanya","Ramya","Kirutika","Akash"};
		String[] phoneContcts = {"Chitti","Mohan","Madan","Pawan","Chityala","Jaya"};
		
		Set<String> unique = new HashSet();
		
		for(String contact:simContcts ) {
			unique.add(contact)
		}
		
	}

}
